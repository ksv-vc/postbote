{
  "name": "@postbote/mjml-react",
  "type": "module",
  "version": "0.2.1",
  "license": "unlicensed",
  "author": {
    "name": "Nick Schmidt",
    "email": "nick@fintory.com"
  },
  "homepage": "https://postbote.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksv-vc/postbote.git",
    "directory": "packages/mjml-react"
  },
  "source": "lib/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "dist/index.js",
      "require": "dist/index.cjs"
    },
    "./extensions/*": {
      "import": "./dist/extensions/*.js",
      "require": "./dist/extensions/*.cjs"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "rm -rf node_modules/ dist/",
    "prepare": "pnpm run build",
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "lib/index.ts",
      "lib/utils.ts",
      "lib/extensions/*.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "external": [
      "color"
    ],
    "clean": true
  },
  "devDependencies": {
    "@postbote/eslint-config": "workspace:*",
    "@postbote/tsconfig": "workspace:*",
    "@types/html-minifier": "^4.0.2",
    "@types/mjml": "4.7.0",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "babel-plugin-transform-runtime": "6.23.0",
    "mjml": "4.13.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "6.2.3"
  },
  "peerDependencies": {
    "mjml": "^4.13.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@babel/runtime": "7.14.6",
    "babel-runtime": "6.25.0",
    "color": "4.2.3",
    "prop-types": "15.8.1"
  }
}
